{
  "name": "Canvas Styler",
  "description": "Make Canvas cooler",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],

  "content_scripts": [
    {
      "matches": ["*://canvas.cornell.edu/*"],
      "css": ["pronhub.css"],
      "js": ["script.js"]
    }
  ]
}
